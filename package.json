{
  "name": "typescript-nextjs-starter",
  "version": "1.0.0",
  "description": "A TypeScript starter for Next.js that includes all you need to build amazing projects",
  "private": true,
  "type": "module",
  "main": "dist/main/main.js",
  "author": {
    "name": "Nagy Viktor",
    "email": "vnagy.kssk@kroschu.com",
    "url": "https://github.com/nagyv"
  },
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "concurrently -k -n \"NEXT,ELECTRON\" -c \"yellow,blue\" --kill-others \"next dev\" \"npx wait-on http://localhost:3000 && electron .\"",
    "build:renderer": "next build && next export",
    "build:electron": "tsc -p tsconfig.electron.json",
    "build": "npm run build:renderer && npm run build:electron && electron-builder --linux --arm64 AppImage",
    "start": "electron .",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"src/**/*.+(ts|js|tsx)\" --fix",
    "format": "prettier . --write"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.3",
    "@supabase/supabase-js": "^2.49.8",
    "@t3-oss/env-nextjs": "0.13.6",
    "electron-serve": "^1.2.0",
    "next": "15.3.3",
    "next-seo": "^6.5.0",
    "next-themes": "^0.3.0",
    "pg": "^8.16.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "zod": "3.25.42"
  },
  "devDependencies": {
    "@types/pg": "^8.15.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "^19.1.6",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "electron": "^36.5.0",
    "electron-builder": "^26.0.12",
    "eslint": "9.27.0",
    "husky": "9.1.7",
    "postcss": "^8.4.38",
    "prettier": "3.5.3",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "5.8.3",
    "wait-on": "^8.0.3"
  },
  "build": {
    "appId": "com.nagyv.typescriptnextjsstarter",
    "productName": "Wireless KFB",
    "directories": {
      "buildResources": "assets"
    },
    "files": [
      "out/**/*",
      "dist/main/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "assets/icon.png",
        "to": "assets/icon.png"
      }
    ],
    "linux": {
      "icon": "assets",
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "arm64"
          ]
        }
      ]
    }
  }
}
